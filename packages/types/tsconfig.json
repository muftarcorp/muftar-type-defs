{
    "compilerOptions": {
      "declaration": true,
      "declarationMap": true,
      "outDir": "dist",
      "module": "CommonJS",
      "target": "ES2020",
      "moduleResolution": "Node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules", "dist"]
  }
  